{
  "expo": {
    "name": "connectify",
    "description": "Cross-platform mobile application connecting and finding events",
    "slug": "connectify-app",
    "privacy": "public",
    "version": "0.1.2",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow connectify to access your photos to add to locations."
        }
      ]
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/map.png",
    "splash": {
      "image": "./assets/prana_splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.grigsby9.prana",
      "buildNumber": "0.1.27",
      "supportsTablet": true,
      "icon": "./assets/map.png"
    },
    "android": {
      "package": "com.grigsby9.prana",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_map.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATIONS",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8aed685d-74b7-49d1-9ed3-6fb4180ef9bd"
      }
    }  
  }
}
